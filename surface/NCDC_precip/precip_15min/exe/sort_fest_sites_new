ascending	# Key 1 direction
fixed		#	location
50		#	starting column
5		#	length
alpha		#	format
none		#	space/tab trimming
no		#	case-folded
ascending	# Key 2 direction
fixed		#	location
56		#	starting column
6		#	length
alpha		#	format
none		#	space/tab trimming
no		#	case-folded
ascending	# Key 3 direction
fixed		#	location
41		#	starting column
8		#	length
alpha		#	format
none		#	space/tab trimming
no		#	case-folded
file		# Output
